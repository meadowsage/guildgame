<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.meadowsage.guildgame.mapper.ApplicantMapper">
    <insert id="insert">
        INSERT INTO applicant (person_id)
        VALUES (#{id})
        ON CONFLICT DO NOTHING
    </insert>
    <delete id="delete">
        DELETE
        FROM applicant
        WHERE person_id = #{personId}
    </delete>
</mapper>